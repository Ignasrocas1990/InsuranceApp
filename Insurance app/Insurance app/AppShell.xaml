             xmlns:pages="clr-namespace:Insurance_app.Pages;assembly=Insurance app"
             x:Class="Insurance_app.AppShell">
    

        <ShellContent ContentTemplate="{DataTemplate pages:LogInPage}" Route="LogInPage"/>
        
    
